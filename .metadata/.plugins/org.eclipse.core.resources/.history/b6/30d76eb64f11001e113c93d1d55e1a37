package br.com.fj.pooneliaalves.util;

import br.com.fj.pooneliaalves.entities.Bank;

public class Transactions {
	private static  double FEE = 5.00;
	
	Bank acc = new Bank();
	double balance = acc.getBalance();
	
	public void withdraw(double withdraw) {
		acc.setBalance(balance - withdraw - FEE);
	}
	
	public void deposit(double deposit) {
		acc.setBalance(acc.getBalance() + deposit);
	}
}
